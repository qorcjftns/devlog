	function fix_height(wrap, ids) {
		var height;
		for (var i = 0; i < wrap.length; i++) {
			if ($(wrap[i])) {
				if (document.all) {
       					height = $(wrap[i]).offsetHeight;
				} else {
       					height = $(wrap[i]).clientHeight;
				}
				/* reset position:relative of the content_project_sub. See [#3822] */
				$(wrap[i]).style.position = 'static';
				break;
			}
		}
		var del = 0;
		for (var i = 0; i < ids.length; i++) {
			if ($(ids[i])) {
        			var h = $(ids[i]).offsetHeight;
				if (del == 0) del = h - height;
				$(ids[i]).style.height = (height) + 'px';
				fake_pad = parseInt($(ids[i]).style.paddingBottom) + parseInt($(ids[i]).style.marginBottom);
				if (fake_pad > 0) {
					$(ids[i]).style.paddingBottom = fake_pad;
				} else {
					$(ids[i]).style.paddingBottom = '30px';
				}
				$(ids[i]).style.marginBottom = 0;
			}
		}
	}
        var oldOnload = window.onload;
        if (typeof window.onload != 'function') {
                window.onload = function() {
			fix_height(wrap, ids);
			fix_webkit();
                }
        } else {
                window.onload = function() {
                        oldOnload();
			fix_height(wrap, ids);
			fix_webkit();
                }
        }
})();
